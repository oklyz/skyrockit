<link rel="stylesheet" href="/stylesheets/style.css" />
<%- include("../partials/_nav") %>
<div class="all-apps-body">
  <div class="all-apps-header">
    <h1>You Apps</h1>
    <a class="add-app-link" href="/users/<%=user._id%>/application/new"></a>
  </div>
  <ul>
    <% applications.forEach(app => { %>
    <a href="/users/<%= user._id %>/application/<%= app._id %>">
      <li><%= app.title %> at <%= app.company %></li>
    </a>
    <% }) %>
  </ul>
</div>
